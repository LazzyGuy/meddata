            <ArticleTitle>NCCN Guidelines Insights: B-Cell Lymphomas, Version 3.2019.</ArticleTitle>
            <Pagination>
                <MedlinePgn>650-661</MedlinePgn>
            </Pagination>
            <ELocationID EIdType="doi" ValidYN="Y">10.6004/jnccn.2019.0029</ELocationID>
