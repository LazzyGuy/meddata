            <ArticleTitle>Could iPSCs Enable &quot;Off-the-Shelf&quot; Cell Therapy?</ArticleTitle>
            <Pagination>
                <MedlinePgn>1667-1669</MedlinePgn>
            </Pagination>
            <ELocationID EIdType="pii" ValidYN="Y">S0092-8674(19)30614-2</ELocationID>
